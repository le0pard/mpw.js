    "postcss-loader": "2.0.9",
    "postcss-reporter": "5.0.0",
    "postcss-url": "7.3.0",
    "promise-polyfill": "7.0.0",
    "prop-types": "15.6.0",
    "query-string": "5.0.1",
    "raven-js": "3.21.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.5",
    "redux": "3.7.2",
    "redux-act": "1.5.1",
    "redux-form": "7.2.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "reselect": "^3.0.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "text-encoding": "^0.6.4",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-manifest-plugin": "^1.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react"
    ],
    "plugins": [
      "syntax-class-properties",
      "syntax-export-extensions",
      "syntax-object-rest-spread",
      "transform-runtime",
      "transform-class-properties",
      "transform-export-extensions",
      "transform-object-rest-spread",
      "transform-regenerator"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "es2015"
          ],
          "react"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "8.1.2",
    "babel-jest": "22.0.4",
    "eslint": "4.14.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-redux-saga": "0.6.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "4.0.0",
    "gulp-jest": "^3.0.1",
    "gulp-stylelint": "6.0.0",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-util": "^3.0.8",
    "jest": "22.0.4",
    "jest-cli": "22.0.4",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0",
    "stylelint-order": "0.8.0",
    "sugarss": "1.0.1"
  },
  "jest": {
    "rootDir": "webpack",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "webpack"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
